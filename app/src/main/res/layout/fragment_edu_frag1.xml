<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:card_view="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
android:orientation="vertical"
tools:context="com.kesteli.filip.sciencebrowser1.education.EduFrag1">

<android.support.design.widget.CoordinatorLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="72dp"
    android:background="@color/white">

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/FABeurekaEdu"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_gravity="bottom|end"
        android:layout_margin="@dimen/fab_margin"
        android:src="@drawable/ic_wb_sunny_white_24dp"
        app:backgroundTint="@color/yellow_500" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/FABfavoriteEdu"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_gravity="bottom|center"
        android:layout_margin="@dimen/fab_margin"
        android:src="@drawable/ic_favorite_white_24dp"
        app:backgroundTint="@color/pink_A400" />

    <android.support.design.widget.FloatingActionButton
        android:id="@+id/FABhistoryEdu"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_gravity="bottom|start"
        android:layout_margin="@dimen/fab_margin"
        android:src="@drawable/ic_history_white_24dp"
        app:backgroundTint="@color/green_A700" />

</android.support.design.widget.CoordinatorLayout>

</LinearLayout>